[
    {
        "CreateComponents": {
            "path": [
                "Collision",
                "QuadTree"
            ],
            "components": [
                {
                    "type": "QuadTree",
                    "name": "root",
                    "parameters": {
                        "path": [
                            "Collision",
                            "QuadTree"
                        ],
                        "max_objects": 32,
                        "max_trees": 256
                    }
                }
            ]
        }
    },
    {
        "CreateComponents": {
            "path": [
                "Collision"
            ],
            "components": [
                {
                    "type": "ColliderMask",
                    "name": "mask",
                    "parameters": {
                        "ids": [
                            "action_space",
                            "actionable",
                            "attack",
                            "both",
                            "player",
                            "physical",
                            "inactive",
                            "check_action"
                        ],
                        "collide_map": {
                            "physical": [
                                "physical",
                                "both",
                                "player"
                            ],
                            "both": [
                                "physical",
                                "both",
                                "player"
                            ],
                            "player": [
                                "physical",
                                "action_space",
                                "both"
                            ],
                            "check_action": [
                                "actionable",
                                "both"
                            ]
                        }
                    }
                },
                {
                    "type": "ColliderVector",
                    "name": "moveable_colliders"
                },
                {
                    "type": "ColliderVector",
                    "name": "static_colliders"
                }
            ]
        }
    },
    {
        "CreateComponents": {
            "path": [
                "Collision",
                "Update"
            ],
            "components": [
                {
                    "type": "System::BroadPhaseCollision",
                    "name": "system-broad_phase_collision",
                    "parameters": {
                        "tree": [
                            "Collision",
                            "QuadTree",
                            "root"
                        ]
                    }
                },
                {
                    "type": "System::ClearQuadtree",
                    "name": "clear_tree",
                    "parameters": {
                        "quadtree": [
                            "Collision",
                            "QuadTree",
                            "root"
                        ]
                    }
                }
            ]
        }
    }
]